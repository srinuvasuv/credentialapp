<!DOCTYPE html>
<html>
<head>
  <title>Demoapp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  
</head>
<body>

<div class="container">
	<div class="row row_center">
	  <div class="col-xs-6">
	  <h1>Demo App</h1>
	    	
	  </div>
	  
	  <div class="col-xs-6">
	  	<h1>
	  	<% if session[:userid]  %>
		
	  	<%= link_to "Log out", logout_path %>
	  		|
	  	<% else %>
	  	
	  		<%= link_to "Login", login_path() %>|
	  	<% end %>
	  		<%= link_to "SignUp", register_path %>
	  		
	  	</h1>
	  </div>
	</div>
</div>

<nav class="navbar navbar-default">
	<div class="container-fluid">
		
	</div>
	
</nav>


  <%= yield %>

<footer class="footer">
  <div class="container row_center">
    <p class="text-muted footer_text">Copy rights 2014.</p>
  </div>
</footer>




</body>
</html>
